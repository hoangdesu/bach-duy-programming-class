<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<Pane xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.hellojavafx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText"/>
    <Button text="Click me!!!" onAction="#onHelloButtonClick"/>

    <Label text="=Do you love me?" />
    <Button text="YESSSS!!!" layoutX="100" layoutY="200" />
    <Button fx:id="noButton" text="No" onMouseEntered="#moveRandom" />

</Pane>
